{
    "name": "tk-framework",
    "version": "6.1.0",
    "license": "MIT",
    "author": "Davide Violante",
    "description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
    "engines": {
        "node": "8.11.1",
        "npm": "6.4.0"
    },
    "scripts": {
        "ng": "ng",
        "build": "ng build --prod",
        "start": "node dist/server/app.front.js",
        "predev": "tsc -p server",
        "predev:back": "tsc -p server",
        "dev": "concurrently \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon --inspect dist/server/app.front.js\"",
        "dev:front": "ng serve --proxy-config proxy.conf.json --open",
        "dev:back": "concurrently \"tsc -w -p server\" \"nodemon --inspect dist/server/app.front.js\"",
        "prod": "concurrently \"ng build --aot --prod && tsc -p server && node dist/server/app.front.js\"",
        "test": "ng test",
        "testbe": "tsc -p server && mocha dist/server/test --exit",
        "lint": "ng lint",
        "linthtml": "htmlhint \"client/**/*.html\"",
        "lintscss": "sass-lint \"client/**/*.scss\" -v",
        "lintbe": "tslint server/**/**.ts{,x}",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.1.0",
        "@angular/common": "^6.1.0",
        "@angular/compiler": "^6.1.0",
        "@angular/core": "^6.1.0",
        "@angular/forms": "^6.1.0",
        "@angular/http": "^6.1.0",
        "@angular/platform-browser": "^6.1.0",
        "@angular/platform-browser-dynamic": "^6.1.0",
        "@angular/router": "^6.1.0",
        "@auth0/angular-jwt": "^2.0.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "bootstrap": "^4.1.3",
        "core-js": "^2.5.4",
        "cors": "^2.8.5",
        "deep-diff": "^1.0.2",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "express-graphql": "^0.7.1",
        "express-remove-route": "^1.0.0",
        "font-awesome": "^4.7.0",
        "graphql": "^14.0.2",
        "jquery": "^3.3.1",
        "jsonwebtoken": "^8.3.0",
        "mongoose": "^5.2.14",
        "mongoose-dynamic-schemas": "^1.2.6",
        "morgan": "^1.9.1",
        "ngx-bootstrap": "^3.1.1",
        "popper.js": "^1.14.4",
        "rxjs": "~6.2.0",
        "rxjs-compat": "^6.2.0",
        "tether": "^1.4.4",
        "underscore": "^1.9.1",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.8.0",
        "@angular/cli": "~6.2.1",
        "@angular/compiler-cli": "^6.1.0",
        "@angular/language-service": "^6.1.0",
        "@types/chai-http": "^3.0.5",
        "@types/express": "^4.16.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "chai": "^4.1.2",
        "chai-http": "^4.2.0",
        "codelyzer": "~4.3.0",
        "concurrently": "^3.6.1",
        "htmlhint": "^0.10.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.4",
        "protractor": "~5.4.0",
        "sass-lint": "^1.12.1",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~2.9.2"
    }
}
